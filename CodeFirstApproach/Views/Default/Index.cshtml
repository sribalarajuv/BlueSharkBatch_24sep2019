@model List<CodeFirstApproach.Models.EmployeeModel>
@{
    ViewBag.Title = "Index";
}

<h2>Index</h2>


<table>
    <tr>
        <th>EmpId</th>
        <th>EmpName</th>
        <th>EmpSalary</th>
        <th>DeptId</th>
    </tr>
    @foreach (var item in Model)
    {
        <tr>
            <td>@item.Empid</td>
            <td>@item.EmpName</td>
            <td>@item.EmpSalary</td>
            <td>@item.DeptId</td>
        </tr>
    }
</table>
